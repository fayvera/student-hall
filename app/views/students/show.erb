<h1><%= @student.first_name %> <%= @student.last_name %></h1>

<h3>Here are <%= @student.first_name %>'s resourses:</h3>
    <ul>
        <% @student.posts.each do |post| %>
            <h4><a href="/posts/<%= post.id %>"> <%= post.title %> </a></h4>
            <%= post.content %>
            <% end %>
    </ul>

    <a href="/posts/new">Create a Post</a> <br></br>

    <a href="/posts">All Publications</a><br></br>
    
    <a href="/logout">Log Out</a>